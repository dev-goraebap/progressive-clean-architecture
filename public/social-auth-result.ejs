<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>소셜인증완료</title>
</head>

<body>
    <div>소셜인증완료</div>
    <script>
        window.onload = function () {
            const provider = '<%= provider %>';
            const token = '<%= token %>';
            const errMsg = '<%= errMsg %>';
            window.opener.postMessage({
                provider,
                token,
                errMsg
            }, '*');
            window.close();
        }
    </script>
</body>

</html>